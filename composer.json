{
    "name": "crysalead/resource",
    "description": "Resource Routing & Dispatching Framework",
    "keywords": ["Resource", "Dispatching"],
    "license": "MIT",
    "require": {
        "php": ">=5.5",
        "crysalead/chaos": "dev-master",
        "crysalead/inflector": "^2.0",
        "crysalead/net": "dev-master",
        "crysalead/router": "~3.0",
        "psr/http-message": "~1.0"
    },
    "require-dev": {
        "crysalead/chaos-database": "dev-master",
        "crysalead/kahlan": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Lead\\Resource\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lead\\Resource\\Spec\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev"
}
